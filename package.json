{
  "name": "doughy-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "npx expo start --port 8081 --localhost",
    "start:claw": "npm start --prefix apps/the-claw-app -- --port 8082 --localhost",
    "start:callpilot": "npm start --prefix apps/callpilot -- --port 8083 --localhost",
    "start:fresh": "npx expo start --port 8081 --clear --localhost",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "postinstall": "patch-package",
    "prepare": "husky || true",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2 --watchAll=false",
    "test:admin": "jest --coverage --collectCoverageFrom='src/features/admin/**/*.{ts,tsx}' --collectCoverageFrom='src/routes/AdminNavigator.tsx'",
    "lint": "expo lint",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check && npm test",
    "build:web": "expo export -p web",
    "build:ios:dev": "APP_VARIANT=development eas build --platform ios --profile development --local",
    "build:ios:stage": "APP_VARIANT=staging eas build --platform ios --profile staging",
    "build:ios:prod": "APP_VARIANT=production eas build --platform ios --profile production",
    "build:android:dev": "APP_VARIANT=development eas build --platform android --profile development --local",
    "build:android:stage": "APP_VARIANT=staging eas build --platform android --profile staging",
    "build:android:prod": "APP_VARIANT=production eas build --platform android --profile production",
    "submit:ios": "eas submit --platform ios --profile production --latest",
    "submit:android": "eas submit --platform android --profile production --latest",
    "deploy:functions:stage": "supabase functions deploy --project-id lqmbyobweeaigrwmvizo",
    "deploy:functions:prod": "supabase functions deploy --project-id vpqglbaedcpeprnlnfxd",
    "db:types": "npx supabase gen types typescript --project-id lqmbyobweeaigrwmvizo --schema public > src/integrations/supabase/types/generated.ts",
    "db:types:stage": "supabase gen types typescript --project-id lqmbyobweeaigrwmvizo > src/integrations/supabase/types/generated.ts",
    "db:types:prod": "supabase gen types typescript --project-id vpqglbaedcpeprnlnfxd > src/integrations/supabase/types/generated.ts",
    "db:lint": "npx supabase db lint",
    "db:diff": "npx supabase db diff --schema public",
    "db:push": "npx supabase db push",
    "db:push:stage": "supabase db push --project-id lqmbyobweeaigrwmvizo",
    "db:push:prod": "supabase db push --project-id vpqglbaedcpeprnlnfxd"
  },
  "dependencies": {
    "@callstack/liquid-glass": "^0.7.0",
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/slider": "5.0.1",
    "@supabase/supabase-js": "^2.90.1",
    "@tanstack/react-query": "^5.90.19",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "expo": "~54.0.33",
    "expo-auth-session": "~7.0.10",
    "expo-av": "^16.0.8",
    "expo-blur": "~15.0.8",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "^18.0.13",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "^3.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.8",
    "expo-sharing": "~14.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "lodash": "^4.17.21",
    "lucide-react-native": "^0.562.0",
    "nativewind": "^4.2.1",
    "papaparse": "^5.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.19",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/papaparse": "^5.5.2",
    "@types/react": "~19.1.0",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "babel-preset-expo": "^54.0.9",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-expo": "~54.0.17",
    "patch-package": "^8.0.1",
    "react-native-worklets": "0.5.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
