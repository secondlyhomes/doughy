{
  "product": {
    "@context": "https://schema.org",
    "@type": "Product",
    "name": "[PRODUCT-NAME]",
    "description": "[PRODUCT-DESCRIPTION]",
    "image": ["https://[YOUR-DOMAIN]/images/product.jpg"],
    "brand": {
      "@type": "Brand",
      "name": "[BRAND-NAME]"
    },
    "sku": "[SKU]",
    "offers": {
      "@type": "Offer",
      "url": "https://[YOUR-DOMAIN]/products/[PRODUCT-SLUG]",
      "priceCurrency": "USD",
      "price": "[PRICE]",
      "availability": "https://schema.org/InStock",
      "priceValidUntil": "[YYYY-MM-DD]"
    },
    "aggregateRating": {
      "@type": "AggregateRating",
      "ratingValue": "[RATING]",
      "reviewCount": "[COUNT]"
    }
  },
  "organization": {
    "@context": "https://schema.org",
    "@type": "Organization",
    "name": "[STORE-NAME]",
    "url": "https://[YOUR-DOMAIN]",
    "logo": "https://[YOUR-DOMAIN]/logo.png",
    "contactPoint": {
      "@type": "ContactPoint",
      "telephone": "[PHONE]",
      "contactType": "customer service"
    },
    "sameAs": [
      "https://facebook.com/[PAGE]",
      "https://instagram.com/[HANDLE]"
    ]
  },
  "breadcrumb": {
    "@context": "https://schema.org",
    "@type": "BreadcrumbList",
    "itemListElement": [
      {
        "@type": "ListItem",
        "position": 1,
        "name": "Home",
        "item": "https://[YOUR-DOMAIN]"
      },
      {
        "@type": "ListItem",
        "position": 2,
        "name": "[CATEGORY]",
        "item": "https://[YOUR-DOMAIN]/[CATEGORY-SLUG]"
      }
    ]
  },
  "faq": {
    "@context": "https://schema.org",
    "@type": "FAQPage",
    "mainEntity": [
      {
        "@type": "Question",
        "name": "What is your return policy?",
        "acceptedAnswer": {
          "@type": "Answer",
          "text": "[YOUR-RETURN-POLICY]"
        }
      },
      {
        "@type": "Question",
        "name": "How long does shipping take?",
        "acceptedAnswer": {
          "@type": "Answer",
          "text": "[YOUR-SHIPPING-INFO]"
        }
      }
    ]
  }
}
