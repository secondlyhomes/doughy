{
  "expo": {
    "name": "The Claw",
    "slug": "the-claw-app",
    "version": "0.1.0",
    "orientation": "portrait",
    "scheme": "theclaw",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theclaw.app"
    },
    "android": {
      "package": "com.theclaw.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "react-native-bottom-tabs",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ef4040",
          "defaultChannel": "default"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b062be67-f94b-4995-a04b-c8c4a364dde4"
      }
    },
    "owner": "dinothesecond"
  }
}
